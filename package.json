{
  "name": "3-api-solid",
  "version": "1.0.0",
  "description": "GymPass style app.",
  "main": "index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "build": "tsup src --out-dir build --external **/*.spec.ts",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment --silent",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:all": "vitest run --dir src/http && vitest run --dir src/use-cases",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "9.19.0",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "22.12.0",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "3.0.5",
    "@vitest/ui": "3.0.5",
    "globals": "15.14.0",
    "prisma": "6.3.0",
    "supertest": "7.0.0",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.22.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.5",
    "vitest-environment-prisma": "0.4.2"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/jwt": "9.0.4",
    "@prisma/client": "6.3.0",
    "@rocketseat/eslint-config": "3.0.12",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.13",
    "dotenv": "16.4.7",
    "eslint": "9.19.0",
    "fastify": "5.2.1",
    "npm-run-all": "4.1.5",
    "tsup": "8.4.0",
    "zod": "3.24.1",
    "esbuild": "0.23.1"
  }
}
